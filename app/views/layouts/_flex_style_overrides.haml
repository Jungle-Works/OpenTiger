:css
 
  #sidewinder-wrapper{
    background: rgba(203,203,210,.15);
  }
  .page-content{
    padding-top: 0;
    background: transparent;
  }
  .wrapper{
    background: transparent;
    margin: 0px;
    max-width: 100%;
  }
  .left-navi{
    width: 230px;
    margin-top: -19px;
    background: #fff;
    margin-bottom: 0px;
    box-shadow: 0 16px 38px -12px rgba(0,0,0,.56), 0 4px 25px 0 rgba(0,0,0,.12), 0 8px 10px -5px rgba(0,0,0,.2);
  }
  .admin-left-navi-link-group-title{
    padding-left: 15px;
    border-radius: 0px;
    color: #fff;
    background-color: ##{ ColorUtils.darken(color1, 15) };
  }
  .admin-left-navi-link-group a{
      margin: 5px 15px;
      opacity: .86;
      border-radius: 0px;
      font-size: 14px;
      font-weight: 400;
      color: #939393;
     
    
  }
  .admin-left-navi-link-group a .left-navi-link-icon{
        color: ##{color1};
  }
  .admin-left-navi-link-group  a:hover{
        color: ##{color1};
  }
  .left-navi-section{
    border-radius: 4px;
    background-color: #fff;
    box-shadow: 0 1px 4px 0 rgba(0,0,0,.14);
    margin-bottom: 30px;
    padding: 15px;
  }

  .left-navi-section form .row .col-6 input{border-radius: 0px;}
  .left-navi-section form .status-select-button{border-radius: 0px;}
  .left-navi-section form .row .col-3 input {border-radius: 0px;}
  .left-navi-section form .row .col-3 a {margin-left: 20px;}
  .left-navi-section table {background-color: transparent;}
  .left-navi-section table thead{font-weight: 400;color: #252422;}
  .left-navi-section table thead th .sort-text{color: #252422;}
  .left-navi-section table thead tr:nth-child(odd){background-color: #fff;}
  .left-navi-section table tr:hover{background-color: #f5f5f5;}
  .left-navi-section table tr:nth-child(odd){background-color: #f9f9f9;}
  .left-navi-section table tr:nth-child(odd):hover{background-color: #f5f5f5;}
  .left-navi-section table th{font-weight: 400;}
  .left-navi-section table td{border-top: 1px solid #ddd;font-size: 14px;}
  .left-navi-section table tr:hover{background-color: #f5f5f5;}
  .left-navi-section h2{
      font-size: 1.375em;
      line-height: 1.09091em;
      font-weight: 600;
  }
  @media screen and (min-width: 768px){
    .wrapper{
      padding: 30px 0px 0px 0px;
      min-height: 1450px;
    }
  }
  
  @media screen and (min-width: 1008px){
    .left-navi-section{
     margin-left: 16.75em;
     margin-right: 1.75em;
    }
  }
